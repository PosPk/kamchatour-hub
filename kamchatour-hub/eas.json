{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_UPDATES_CHANNEL": "development"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_UPDATES_CHANNEL": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_UPDATES_CHANNEL": "staging"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_UPDATES_CHANNEL": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}