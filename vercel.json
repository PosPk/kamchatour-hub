{
  "version": 2,
  "builds": [
    { "src": "api/**/*.ts", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" },
    { "src": "new-web/package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/api/webhooks/cloudpays", "dest": "/api/webhooks/cloudpays.ts" },
    { "src": "/api/health", "dest": "/api/health.ts" },
    { "src": "/api/env", "dest": "/api/env.ts" },
    { "src": "^/tg$", "dest": "/public/tg/index.html" },
    { "src": "^/tg/(.*)$", "dest": "/public/tg/$1" },
    { "handle": "filesystem" }
  ]
}

