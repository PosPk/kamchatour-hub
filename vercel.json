{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "installCommand": "cd new-web && npm ci --no-audit --no-fund --omit=dev",
  "buildCommand": "cd new-web && npm install --no-audit --no-fund && npm run build",
  "outputDirectory": "new-web/out",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "handle": "filesystem" }
  ]
}
