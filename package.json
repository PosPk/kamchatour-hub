{
  "name": "mobile-new",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export -p web && rm -rf public && mv dist public && cp -f static/robots.txt public/robots.txt && cp -f public/index.html public/error.html",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "jest",
    "doctor": "expo-doctor",
    "clean": "rm -rf dist public"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "expo": "53.0.22",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.10",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.1.1"
  },
  "engines": {
    "node": ">=18 <21",
    "npm": ">=9"
  },
  "private": true
}
